{
  "name": "clip-tokenizer-rs",
  "version": "0.1.0",
  "type": "module",
  "description": "Fast CLIP tokenizer using Rust via napi-rs",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nstandif/clip-tokenizer-rs.git"
  },
  "bugs": {
    "url": "https://github.com/nstandif/clip-tokenizer-rs/issues"
  },
  "homepage": "https://github.com/nstandif/clip-tokenizer-rs#readme",
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ],
  "napi": {
    "name": "clip_tokenizer",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "aarch64-unknown-linux-gnu"
    ]
  },
  "scripts": {
    "build": "napi build --release --platform --strip",
    "build:debug": "napi build --platform",
    "test": "bun test",
    "generate-fixtures": "bun scripts/generate-fixtures.ts",
    "ci:publish": "napi artifacts && npm publish --access public --provenance"
  },
  "devDependencies": {
    "@huggingface/transformers": "^3.8.1",
    "@napi-rs/cli": "^3.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "semantic-release": "^24.2.0"
  }
}
